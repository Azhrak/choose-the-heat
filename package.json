{
  "name": "choose-the-heat",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite dev --port 3000 --host",
    "build": "vite build",
    "start": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "biome check . && markdownlint-cli2",
    "lint:fix": "biome check --write . && markdownlint-cli2 --fix",
    "lint:md": "markdownlint-cli2",
    "lint:md:fix": "markdownlint-cli2 --fix",
    "format": "biome format --write .",
    "docs:validate": "tsx scripts/validate-docs.ts",
    "db:migrate": "tsx --env-file=.env src/lib/db/migrate.ts",
    "db:codegen": "kysely-codegen --out-file=src/lib/db/types.ts",
    "db:seed": "tsx --env-file=.env src/lib/db/seed.ts",
    "cleanup:audit-logs": "tsx src/lib/jobs/cleanup-audit-logs.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.1",
    "@ai-sdk/google": "^3.0.1",
    "@ai-sdk/mistral": "^3.0.1",
    "@ai-sdk/openai": "^3.0.1",
    "@google-cloud/storage": "^7.18.0",
    "@google-cloud/text-to-speech": "^6.4.0",
    "@google/genai": "^1.34.0",
    "@node-rs/argon2": "^2.0.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-devtools": "^0.9.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-router": "^1.143.11",
    "@tanstack/react-router-devtools": "^1.143.11",
    "@tanstack/react-router-ssr-query": "^1.143.11",
    "@tanstack/react-start": "^1.143.11",
    "@tanstack/router-plugin": "^1.143.11",
    "ai": "^6.0.3",
    "arctic": "^3.7.0",
    "clsx": "^2.1.1",
    "kysely": "^0.28.9",
    "lucide-react": "^0.562.0",
    "openai": "^6.15.0",
    "pg": "^8.13.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "stripe": "^20.1.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "vite-tsconfig-paths": "^6.0.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "dotenv": "^17.2.3",
    "encoding": "^0.1.13",
    "h3": "2.0.1-rc.6",
    "jsdom": "^27.4.0",
    "kysely-codegen": "^0.19.0",
    "markdownlint-cli2": "^0.20.0",
    "nitro": "3.0.1-alpha.1",
    "postcss": "^8.4.49",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "web-vitals": "^5.1.0"
  }
}
